{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/index.js"
      }
    ],
    "installCommand": "npm install",
    "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }]
  }